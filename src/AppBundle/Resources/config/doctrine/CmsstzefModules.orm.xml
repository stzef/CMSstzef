<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\CmsstzefModules" table="CMSstzef_modules">
    <indexes>
      <index name="fk_CMSstzef_modules_CMSstzef_sections1_idx" columns="id_section_theme"/>
      <index name="fk_CMSstzef_modules_CMSstzef_types_modules1_idx" columns="id_type_module"/>
      <index name="fk_CMSstzef_modules_CMSstzef_states1_idx" columns="id_state_publication"/>
    </indexes>
    <id name="idModule" type="integer" column="id_module">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="name" type="string" column="name" length="45" nullable="false"/>
    <many-to-one field="idStatePublication" target-entity="CmsstzefStatesPublication">
      <join-columns>
        <join-column name="id_state_publication" referenced-column-name="id_state"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idTypeModule" target-entity="CmsstzefTypesModules">
      <join-columns>
        <join-column name="id_type_module" referenced-column-name="id_type_modules"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idSectionTheme" target-entity="CmsstzefSections">
      <join-columns>
        <join-column name="id_section_theme" referenced-column-name="id_section_theme"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
