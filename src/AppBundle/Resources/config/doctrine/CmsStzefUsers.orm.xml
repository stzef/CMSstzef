<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\CmsStzefUsers" table="cms_stzef_users">
    <indexes>
      <index name="fk_CMSstzef_users_CMSstzef_users_groups1_idx" columns="id_users_group"/>
      <index name="fk_CMSstzef_users_CMSstzef_states1_idx" columns="id_state"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="name" type="string" column="name" length="45" nullable="false"/>
    <field name="username" type="string" column="username" length="45" nullable="false"/>
    <field name="email" type="string" column="email" length="45" nullable="true"/>
    <field name="password" type="string" column="password" length="100" nullable="false"/>
    <field name="plainpassword" type="string" column="plainPassword" length="45" nullable="true"/>
    <field name="isactive" type="string" column="isActive" length="45" nullable="false"/>
    <field name="resgiterDate" type="string" column="resgiter_date" length="45" nullable="false"/>
    <field name="activation" type="string" column="activation" length="45" nullable="false"/>
    <many-to-one field="idUsersGroup" target-entity="CmsStzefUsersGroups">
      <join-columns>
        <join-column name="id_users_group" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idState" target-entity="CmsStzefStates">
      <join-columns>
        <join-column name="id_state" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
