{% extends 'base.html.twig' %}

{% block body %}
    <h1>Listado Parametros</h1>

    <table class="table" id="CMSstzef_list">
        <thead>
            <tr>
                <th>Codigo</th>
                <th>Id</th>
                <!--<th>Cgroup</th>-->
                <!--<th>Ngroup</th>-->
                <th>Nombre</th>
                <th>Valor</th>
                <th>Acciones</th>
            </tr>
        </thead>
        <tbody>
        {% for cmsStzefParameter in cmsStzefParameters %}
            <tr>
                <!--<td><a href="{{ path('admstzef_parameters_show', { 'id': cmsStzefParameter.id }) }}">{{ cmsStzefParameter.cparam }}</a></td>-->
                <td>{{ cmsStzefParameter.cparam }}</td>
                <td>{{ cmsStzefParameter.id }}</td>
                <!--<td>{{ cmsStzefParameter.cgroup }}</td>-->
                <!--<td>{{ cmsStzefParameter.ngroup }}</td>-->
                <td>{{ cmsStzefParameter.name }}</td>
                {% if cmsStzefParameter.type == "INT" %}
                    <td>{{ cmsStzefParameter.valueInt }}</td>
                {% elseif cmsStzefParameter.type == "TEXT" %}
                    <td>{{ cmsStzefParameter.valueText }}</td>
                {% elseif cmsStzefParameter.type == "JSON" %}
                    <td>{{ cmsStzefParameter.valueText }}</td>
                {% elseif cmsStzefParameter.type == "BOOL" %}
                    <td>{% if cmsStzefParameter.valueBool %} <b>Si</b> {% else %}No{% endif %}</td>
                {% endif %}
                <td>
                    <ul class="list-unstyled">
                        <!--<li>
                            <a class="btn btn-" role="button" href="{{ path('admstzef_parameters_show', { 'id': cmsStzefParameter.id }) }}">show</a>
                        </li>-->
                        <li>
                            <!--<a class="btn btn-" role="button" href="{{ path('admstzef_parameters_edit', { 'id': cmsStzefParameter.id }) }}">Editar</a>-->
                        </li>
                    </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <ul class="list-unstyled">
        <li>
            <a class="btn btn-success" role="button" href="{{ path('admstzef_parameters_new') }}">Nuevo Parametro</a>
        </li>
    </ul>
{% endblock %}
