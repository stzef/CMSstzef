
{% extends 'base.html.twig' %}

{% block body %}
    <h1>Galeria <b>{{ cmsStzefBanner.name }}</b></h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Nombre</th>
                <td>{{ cmsStzefBanner.name }}</td>
            </tr>
            <tr>
                <th>Principal</th>
                <td>{% if cmsStzefBanner.ifMain %} <b>Si</b> {% else %}No{% endif %}</td>
            </tr>
        </tbody>
    </table>

    <ul class="list-unstyled" >
        <li>
            <a class="btn btn-primary" role="button" href="{{ path('admstzef_banners_index') }}">Listado</a>
        </li>
        <li>
            <a class="btn btn-info" role="button" href="{{ path('admstzef_banners_edit', { 'id': cmsStzefBanner.id }) }}">Editar</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" class="btn btn-danger" value="Borrar">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
