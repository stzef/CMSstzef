{% extends 'base.html.twig' %}

{% block body %}
    <h1>Listado Cmsstzefpages</h1>

    <table class="table" id="CMSstzef_list">
        <thead>
            <tr>
                <th>Name</th>
                <th>Ifmain</th>
                <th>Slug</th>
                <th>Acciones</th>
            </tr>
        </thead>
        <tbody>
        {% for cmsStzefPage in cmsStzefPages %}
            <tr>
                <td><a href="{{ path('admstzef_pages_show', { 'id': cmsStzefPage.slug }) }}">{{ cmsStzefPage.name }}</a></td>
                <td>{% if cmsStzefPage.ifMain %}Yes{% else %}No{% endif %}</td>
                <td>{{ cmsStzefPage.slug }}</td>
                <td>
                    <ul class="list-unstyled">
                        <li>
                            <a class="btn btn-" role="button" href="{{ path('admstzef_pages_show', { 'id': cmsStzefPage.slug }) }}">show</a>
                        </li>
                        <li>
                            <a class="btn btn-" role="button" href="{{ path('admstzef_pages_edit', { 'id': cmsStzefPage.slug }) }}">edit</a>
                        </li>
                    </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <ul class="list-unstyled">
        <li>
            <a class="btn btn-success" role="button" href="{{ path('admstzef_pages_new') }}">Nuevo cmsStzefPage</a>
        </li>
    </ul>
{% endblock %}
