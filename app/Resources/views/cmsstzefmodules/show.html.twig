
{% extends 'base.html.twig' %}

{% block body %}
    <h1>Modulo <b>{{ cmsStzefModule.name }}</b> </h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Nombre</th>
                <td>{{ cmsStzefModule.name }}</td>
            </tr>
            <tr>
                <th>Contenido</th>
                <td>{{ cmsStzefModule.contentHtml|raw }}</td>
            </tr>
            <tr>
                <th>Id</th>
                <td>{{ cmsStzefModule.id }}</td>
            </tr>
        </tbody>
    </table>

    <ul class="list-unstyled" >
        <li>
            <a class="btn btn-primary" role="button" href="{{ path('admstzef_modules_index') }}">Listado</a>
        </li>
        <li>
            <a class="btn btn-info" role="button" href="{{ path('admstzef_modules_edit', { 'id': cmsStzefModule.id }) }}">Editar</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" class="btn btn-danger" value="Borrar">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
